@model IEnumerable<WebShop.Models.PRODUCT_CATEGORIES>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DATE_CREATION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DATE_MODIFI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CATEGORY.NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PRODUCT.NAME)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DATE_CREATION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DATE_MODIFI)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CATEGORY.NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRODUCT.NAME)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
